n=int(input('enter number:'))
for i in range(1,n+1):
  for k in range(n-i,0,-1):
       Print(" ",end=" ")
  for j in range(1,i+1):
      Print("*",end="  ")
Print("\n")
for i in range(1,n+1):
   for k in range(1,i+1):
       Print(" ",end=" ")
   for j in range(n-i,0,-1):
       Print("*",end="  ")
  Print("\n")
